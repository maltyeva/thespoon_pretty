<p id="notice"><%= notice %></p>
<p>
  <strong>Name:</strong>
  <%= @restaurant.name %>
</p>
<p>
  <strong>Address:</strong>
  <%= @restaurant.address %>
</p>
<p>
  <strong>Description:</strong>
  <%= @restaurant.description %>
</p>
<p>
  <strong>Stars:</strong>
  <%= @restaurant.stars %>
</p>
<p>
  <ul>
    <% @restaurant.reviews.each do |review| %>
      <li><%= review.content %></li>
    <% end %>
  </ul>
</p>
<%= link_to 'Edit', edit_restaurant_path(@restaurant) %> |
<%= link_to 'Back', restaurants_path %> | 
<%= link_to "Learn More about the chef", chef_restaurant_path(@restaurant) %>
<p><%= link_to "Add a review!", new_restaurant_review_path(@restaurant)  %></p>